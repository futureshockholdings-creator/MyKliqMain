{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development",
        "API_URL": "https://c7dd138c-576d-4490-a426-c0be6e6124ca-00-1u3lut3kqrgq6.kirk.replit.dev"
      },
      "comment": "Development uses dev URL - this is correct"
      },
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true,
        "bundleIdentifier": "com.mykliq.app.dev"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "applicationId": "com.mykliq.app.dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview",
        "API_URL": "https://api.mykliq.com"
      },
      "ios": {
        "resourceClass": "m-medium",
        "bundleIdentifier": "com.mykliq.app.preview"
      },
      "android": {
        "buildType": "apk",
        "applicationId": "com.mykliq.app.preview"
      }
    },
    "production": {
      "env": {
        "APP_VARIANT": "production",
        "API_URL": "https://api.mykliq.com"
      },
      "ios": {
        "resourceClass": "m-medium",
        "bundleIdentifier": "com.mykliq.app"
      },
      "android": {
        "buildType": "aab",
        "applicationId": "com.mykliq.app"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "REPLACE_WITH_YOUR_APPLE_ID@example.com",
        "ascAppId": "REPLACE_WITH_YOUR_ASC_APP_ID",
        "appleTeamId": "REPLACE_WITH_YOUR_APPLE_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "./path/to/google-service-account.json",
        "track": "production"
      }
    },
    "preview": {
      "ios": {
        "appleId": "REPLACE_WITH_YOUR_APPLE_ID@example.com",
        "ascAppId": "REPLACE_WITH_YOUR_ASC_APP_ID",
        "appleTeamId": "REPLACE_WITH_YOUR_APPLE_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "./path/to/google-service-account.json",
        "track": "internal"
      }
    }
  }
}
